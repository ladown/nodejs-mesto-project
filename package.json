{
  "name": "nodejs-mesto-project",
  "version": "1.0.0",
  "description": "Backend для проекта Mesto, написанный при помощи NodeJS",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev src/app.ts",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "eslint:fix": "npx eslint src/**/*.ts --fix",
    "prettier:write": "npx prettier src/**/*.ts --write",
    "lint": "npm run eslint:fix && npm run prettier:write",
    "prepare": "husky"
  },
  "author": "Egor Ermilov <egorka33rus52@gmail.com> (https://ermilovee.ru)",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "pnpm lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^18.6.3",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "celebrate": "^15.0.3",
    "commitizen": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "winston": "^3.14.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.5.0",
    "validator": "^13.12.0"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
